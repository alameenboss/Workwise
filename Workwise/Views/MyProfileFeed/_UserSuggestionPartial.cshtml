@{
    var username = ViewData["username"] != null ? ViewData["username"].ToString() : string.Empty;

    var heading = username == User.Identity.Name ? "People Viewed Profile" : "Suggestions";
}


<div class="suggestions full-width">
    <div class="sd-title">
        <h3>@heading</h3>
        <i class="fa fa-ellipsis-v"></i>
    </div><!--sd-title end-->
    <div class="suggestions-list">
        @Html.Action("SuggestedUser","Index")
        <div class="view-more">
            <a href="#" title="">View More</a>
        </div>
    </div><!--suggestions-list end-->
</div><!--suggestions end-->
